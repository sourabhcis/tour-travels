x",[],[],"/windows/sourabh 3.0/MernStack-Tour-Management/tour-management/frontend/src/shared/Newsletter.jsx",[],[],"/windows/sourabh 3.0/MernStack-Tour-Management/tour-management/frontend/src/shared/CommonSection.jsx",[],[],"/windows/sourabh 3.0/MernStack-Tour-Management/tour-management/frontend/src/components/Booking/Booking.jsx",["220"],[],"import React from \"react\";\nimport \"./booking.css\";\nimport {\n  Form,\n  FormGroup,\n  ListGroup,\n  ListGroupItem,\n  Button,\n  List,\n} from \"reactstrap\";\nimport { useState } from \"react\";\nimport { useNavigate } from \"react-router-dom\";\n\nconst Booking = ({ tour }) => {\n  const navigate = useNavigate();\n  const { price, avgRating, reviews } = tour;\n  const [booking_FormData, setBooking_Formdata] = useState({\n    userId:'01',\n    userEmail:'example@gmail.com',\n    fullname:'',\n    phone:'',\n    bookAt:'',\n    guestSize:1\n  })\n\n  const handleChange = (e) => {\n    setBooking_Formdata({...booking_FormData, [e.target.name]:e.target.value})\n  };\n\n  let serviceFee = 10;\n  let totalAmount = Number(price * booking_FormData.guestSize) + Number(serviceFee);\n\n  const handle_BookingSubmit = () =>{\n    //console.log(booking_FormData)\n    navigate('/thank-you')\n  }\n\n  return (\n    <div className=\"booking\">\n      {/* Booking top */}\n      <div className=\"booking__top d-flex align-items-center justify-content-between\">\n        <h3>\n          $ {price} <span> /per person</span>{\" \"}\n        </h3>\n        <span>\n          {\" \"}\n          <i class=\"ri-star-fill\"></i> {avgRating} ( {reviews?.length} ){\" \"}\n        </span>\n      </div>\n\n      {/* Booking form*/}\n      <div className=\"booking__form\">\n        <h5>Information</h5>\n        <Form className=\"booking__info-form\">\n          <FormGroup>\n            <input\n              type=\"text\"\n              placeholder=\"Full Name\"\n              name=\"fullname\"\n              required\n              onChange={handleChange}\n            />\n          </FormGroup>\n          <FormGroup>\n            <input\n              type=\"number\"\n              placeholder=\"Phone\"\n              name=\"phone\"\n              required\n              onChange={handleChange}\n            />\n          </FormGroup>\n          <FormGroup className=\"d-flex align-items-center gap-3\">\n            <input\n              type=\"date\"\n              placeholder=\"\"\n              name=\"bookAt\"\n              required\n              onChange={handleChange}\n            />\n            <input\n              type=\"number\"\n              placeholder=\"Guest\"\n              name=\"guestSize\"\n              required\n              onChange={handleChange}\n            />\n          </FormGroup>\n        </Form>\n      </div>\n\n      {/* Booking form bottom */}\n      <div className=\"booking__bottom\">\n        <ListGroup>\n          <ListGroupItem className=\"border-0 px-0\">\n            <h5 className=\"d-flex align-items-center gap-1\">\n              ${price} <i class=\"ri-close-line\"></i> 1 person{\" \"}\n            </h5>\n            <span>$ {price} </span>\n          </ListGroupItem>\n          <ListGroupItem className=\"border-0 px-0\">\n            <h5> Service Charge </h5>\n            <span>$ {serviceFee} </span>\n          </ListGroupItem>\n          <ListGroupItem className=\"border-0 px-0 total\">\n            <h5> Total </h5>\n            <span> $ {totalAmount} </span>\n          </ListGroupItem>\n        </ListGroup>\n\n        <Button className=\"btn primary__btn w-100 mt-4\" onClick={handle_BookingSubmit}>Book Now</Button>\n      </div>\n    </div>\n  );\n};\n\nexport default Booking;\n","/windows/sourabh 3.0/MernStack-Tour-Management/tour-management/frontend/src/pages/Thankyou.jsx",[],[],"/windows/sourabh 3.0/MernStack-Tour-Management/tour-management/frontend/src/pages/About.jsx",["221"],[],"import React from \"react\";\nimport { Container, Row, Col } from \"reactstrap\";\nimport